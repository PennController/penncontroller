            if (_engine_js__WEBPACK_IMPORTED_MODULE_2__["PennEngine"].tmpItems.indexOf(window.items[i])<0)
                _engine_js__WEBPACK_IMPORTED_MODULE_2__["PennEngine"].tmpItems.push(window.items[i]);
