    log: (...messages) => _engine__WEBPACK_IMPORTED_MODULE_0__["PennEngine"].debug.addToTab(debug.logTab.log,...messages),
    error: (...messages) => {
        if (!_engine__WEBPACK_IMPORTED_MODULE_0__["PennEngine"].debug.on) return;
